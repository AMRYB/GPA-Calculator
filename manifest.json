{
  "manifest_version": 3,
  "name": "GPA Calculator",
  "version": "1.2",
  "description": "GPA Calculator for HNU students",
  "permissions": ["scripting", "activeTab"],
  "icons": {
    "128": "/assets/icon.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["assets/*"],
      "matches": ["<all_urls>"]
    }
  ],
  "action": {
    "default_title": "GPA Calculator",
    "default_popup": "popup.html"
  }
}

{
  "manifest_version": 3,
  "name": "GPA Calculator",
  "version": "3.714",
  "description": "GPA Calculator for HNU students",
  "permissions": ["scripting", "activeTab", "storage"],
  "host_permissions": [
    "https://github.com/*",
    "https://api.github.com/*"
  ],
  "icons": {
    "128": "/assets/img/favicon.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["assets/*"],
      "matches": ["<all_urls>"]
    }
  ],
  "action": {
    "default_title": "GPA Calculator",
    "default_popup": "popup.html"
  }
}
